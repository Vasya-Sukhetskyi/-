#include <stdio.h>
#include <math.h>
    //Ці директиви додають необхідні заголовочні файли для використання функцій вводу/виводу (printf та scanf) та математичних операцій (sqrt).
double vector_length(int x1, int y1, int x2, int y2) {
    //Ця функція приймає чотири цілі числа x1, y1, x2 і y2 - координати початку та кінця вектора. Вона обчислює різницю між координатами по осі x (dx) та по осі y (dy), а потім використовує формулу sqrt(dx*dx + dy*dy) для обчислення довжини вектора.
    int dx = x2 - x1;
    
   
int dy = y2 - y1;
    double length = sqrt(dx*dx + dy*dy);
    //Результат повертається як значення типу double.
    return length;
}

int main() {
    int x1, y1, x2, y2;
    printf("Введіть координати початку та кінця вектора (x1, y1, x2, y2): ");
    scanf("%d %d %d %d", &x1, &y1, &x2, &y2);
    
    double length = vector_length(x1, y1, x2, y2);
    //Викликається функція vector_length з цими координатами, і результат, тобто довжина вектора, зберігається у змінній length.
    printf("Довжина вектора: %.6f\n", length);
    //Функція printf виводить довжину вектора з точністю до шести знаків після коми.
    return 0;
}
