#include <stdio.h>

int countSequences(int n) {   
    int mod = 12345;    
    //Модуль mod використовується для обчислення залишку від ділення, щоб уникнути переповнень при великих значеннях.
    int a = 1, b = 2, c = 4;  

    if (n == 1)
        return 2;
    //У рядку if (n == 1) return 2; перевіряється, чи дорівнює n 1. Якщо так, то функція повертає 2, оскільки для n = 1 є дві можливі послідовності: 0 і 1.
    if (n == 2)
        return 4;
    //У рядку if (n == 2) return 4; перевіряється, чи дорівнює n 2. Якщо так, то функція повертає 4, оскільки для n = 2 є чотири можливі послідовності: 00, 01, 10 і 11.
    for (int i = 3; i <= n; i++) {    
    //цикл for проходить від 3 до n і обчислює наступне значення c за допомогою формули (a + b + c) % mod
        int temp = (a + b + c) % mod; 
        a = b;
        b = c;
        c = temp;
    }

    return c;
}

int main() {
    int n;

    printf("Введіть довжину послідовності n: ");
    scanf("%d", &n);

    int result = countSequences(n);

    printf("Кількість шуканих послідовностей: %d\n", result);

    return 0;
}
